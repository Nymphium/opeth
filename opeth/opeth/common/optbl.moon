{
		ADD:    (a, b) -> a +  b
		SUB:    (a, b) -> a -  b
		MUL:    (a, b) -> a *  b
		DIV:    (a, b) -> a /  b
		MOD:    (a, b) -> a %  b
		POW:    (a, b) -> a ^  b

		IDIV:   (a, b) -> a // b
		BAND:   (a, b) -> a &  b
		BOR:    (a, b) -> a |  b
		BXOR:   (a, b) -> a ~  b
		SHL:    (a, b) -> a << b
		SHR:    (a, b) -> a >> b

		LT:     (a, b) -> a <  b
		LE:     (a, b) -> a <= b
		EQ:     (a, b) -> a == b
}

